{
  "name": "construct",
  "version": "1.0.0",
  "description": "A webvr control room with support for spotify, netflix, VNC, Leap motion hand tracking. General environmental controls and interface operation. Websockify binary for windows used from here - https://github.com/downloads/kanaka/websockify/websockify.zip",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"gulp\" \"cd websockify&&websockify.exe 192.168.0.8:8080 192.168.0.8:5900\"",
    "test": "node_modules/.bin/mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaneharris/construct.git"
  },
  "keywords": [
    "webvr",
    "virtual",
    "desktop",
    "vnc"
  ],
  "author": "Shane Harris",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/shaneharris/construct/issues"
  },
  "homepage": "https://github.com/shaneharris/construct#readme",
  "devDependencies": {
    "@types/three": "^0.84.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.12",
    "browserify": "^14.4.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "mocha": "^3.4.2",
    "three.js": "^0.77.1",
    "tsify": "^3.0.1",
    "typescript": "^2.3.3",
    "typings": "^2.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  }
}
